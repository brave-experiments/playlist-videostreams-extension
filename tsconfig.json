{
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "jsx": "react",
    "sourceMap": true,
    "outDir": "bundle",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}